<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.nelg.SpringWeb.dao.AdminDao">
    <!--配置查询所有-->
    <select id="findByUsername" parameterType="java.lang.String" resultType="org.nelg.SpringWeb.entity.Admin">
        SELECT * FROM base_admin WHERE username = #{username}
    </select>
</mapper>